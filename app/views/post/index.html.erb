<h1>Lista Articulos</h1>
    <%= link_to "Dashboard", post_dashboard_path%>
<table class="table">
    <tr>
        <th>Titulo</th>
        <th>Fecha</th>
        <th>Image</th>
        <th>Comentario</th>
    </tr>
    <% @posts.each do |post| %>
    <tr>
        <td><%= post.title %> </td>
        <td><%= post.created_at.strftime("%d-%m-%Y %H:%M") %> </td>
        <td><img src="<%= post.image_url %>" style="width:150px"></td>
        <td><%= post.content %> </td>
    </tr>
    <% end %>
</table>